{
    "name": "ai-ml-tuts",
    "version": "0.1.0",
    "description": "An collection of code snippets to attempt to learn AI & ML in Javascript",
    "source": "src/index.html",
    "scripts": {
        "start": "rm -rf .parcel-cache && parcel src/*.html --public-url / --open",
        "build": "parcel build src/*.html --no-cache --public-url ./",
        "predeploy": "rm -rf dist && parcel build src/*.html --no-cache --public-url ./",
        "deploy": "gh-pages -d dist",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
